#! /bin/sh

set -e

case "$1" in
    configure)
        # Alternatives
        update-alternatives --quiet \
            --install /usr/bin/logo logo /usr/bin/ucblogo 30 \
            --slave \
            /usr/share/man/man1/logo.1.gz logo.1.gz \
            /usr/share/man/man1/ucblogo.1.gz
        ;;
esac

